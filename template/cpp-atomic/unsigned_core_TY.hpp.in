            case memory_order_release:
                return MAP_CAST_TY(__vatomic_xchg_rel(MAP_CAST_VL(&_v), v));
            case memory_order_relaxed:
                return MAP_CAST_TY(__vatomic_xchg_rlx(MAP_CAST_VL(&_v), v));
            case memory_order_consume:
            case memory_order_acquire:
                return MAP_CAST_TY(__vatomic_xchg_acq(MAP_CAST_VL(&_v), v));
            case memory_order_acq_rel:
            case memory_order_seq_cst:

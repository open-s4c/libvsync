    void store(MAP_TMPL_T_TY v, memory_order order = memory_order_seq_cst) MAP_VL noexcept
    {
        switch (order) {
            case memory_order_release:
                __vatomic_write_rel(MAP_CAST_VL(&_v), v);
                break;
            case memory_order_relaxed:
                __vatomic_write_rlx(MAP_CAST_VL(&_v), v);
                break;
            case memory_order_acquire:
            case memory_order_acq_rel:
            case memory_order_consume:
            case memory_order_seq_cst:
            default:
                return __vatomic_write(MAP_CAST_VL(&_v), v);
        }

/*****************************************************************************
 * MultiThreadedTest: __vatomic_OP_MS
 *****************************************************************************/
static inline void *
mt_atomic_OP_MS_run(void *args)
{
    vsize_t tid = (vsize_t)(vuintptr_t)args;
    TT mask    = MAP_MASK_TY;

    for (vsize_t i = 0; i < IT; i++) {
        mask = mask << (i * tid);
        __vatomic_OP_MS(&g_shared, mask);
    }
    V_UNUSED(tid);
    return NULL;
}
static inline void
mt_atomic_OP_MS(void)
{
    TT init     = MAP_VAL_TY;
    TT mask     = MAP_MASK_TY;
    TT expected = MAP_VAL_TY;
    __vatomic_init(&g_shared, init);
    pthread_t t[N];
    for (vsize_t i = 0; i < N; i++)
        pthread_create(&t[i], 0, mt_atomic_OP_MS_run, (void*)i);
    for (vsize_t i = 0; i < N; i++)
        pthread_join(t[i], 0);
    for (vsize_t t = 0; t < N; t++) {
        mask = MAP_MASK_TY;
        for (vsize_t i = 0; i < IT; i++) {
            mask     = mask << (i * t);
            expected = expected MAP_OP mask;
        }
    }
    TT val = __vatomic_read(&g_shared);
    assert(val == expected);
    V_UNUSED(expected, val);
}
_tmpl_end;

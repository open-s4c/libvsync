    for (vsize_t i = 0; i < N; i++)
        pthread_join(t[i], 0);
    /* calculate the expected value */
    TT expected = init MAP_OP(N * IT * MAP_OFFSET_TY);
    TT val      = __vatomic_read(&g_shared);
    assert(expected == val);
    V_UNUSED(expected, val);
}

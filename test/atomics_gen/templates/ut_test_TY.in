static inline void
ut_atomic_TY_and_MS(void)
{
    AA obj  = {MAP_MAX_TY};
    TT mask = MAP_MAX_TY;
    TT ref  = MAP_MAX_TY;
    TT cur  = 0;

    for (vsize_t i = 0; i < sizeof(TT); i++) {
        mask = (TT)(mask >> (BYTE_WIDTH * i));
        ref &= mask;
        _vatomic_and_MS(&obj, mask);
        cur = _vatomic_read(&obj);
        ASSERT(cur == ref);
    }
    V_UNUSED(cur);
}

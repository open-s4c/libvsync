ut_atomic_TY_xor_MS(void)
{
    AA obj  = {MAP_INIT_TY};
    TT mask = 0x33;
    TT ref  = MAP_INIT_TY;
    TT cur  = 0;

    for (vsize_t i = 0; i < sizeof(TT); i++) {
        mask = (TT)(mask << (BYTE_WIDTH * i)) | mask;
        ref ^= mask;
        _vatomic_xor_MS(&obj, mask);
        cur = _vatomic_read(&obj);
        ASSERT(cur == ref);
